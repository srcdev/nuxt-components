{
  "name": "srcdev-nuxt-components",
  "type": "module",
  "version": "8.0.3",
  "main": "nuxt.config.ts",
  "types": "types.d.ts",
  "scripts": {
    "clean": "rm -rf .nuxt && rm -rf .output",
    "cleanall": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm package-lock.json",
    "reinstall": "rm -rf node_modules && npm install",
    "cleaninstall": "npm run clean && npm run reinstall",
    "dev": "nuxi dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "release": "release-it",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui"
  },
  "files": [
    "app/",
    "types/",
    "app/types/components/",
    "nuxt.config.ts",
    "types.d.ts"
  ],
  "devDependencies": {
    "@iconify-json/akar-icons": "1.2.7",
    "@iconify-json/bi": "1.2.7",
    "@iconify-json/bitcoin-icons": "1.2.4",
    "@iconify-json/gravity-ui": "1.2.11",
    "@iconify-json/material-symbols": "1.2.53",
    "@iconify-json/mdi": "1.2.3",
    "@nuxt/eslint": "1.13.0",
    "@nuxt/icon": "2.2.1",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "3.23.0",
    "@nuxtjs/i18n": "10.2.1",
    "@pinia/nuxt": "0.11.3",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "14.2.0",
    "eslint": "9.39.2",
    "focus-trap-vue": "4.1.0",
    "happy-dom": "20.5.0",
    "nuxt": "4.3.0",
    "nuxt-qrcode": "0.4.8",
    "patch-package": "8.0.1",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "release-it": "19.2.4",
    "typescript": "5.9.3",
    "vitest": "^4.0.18",
    "vue-tsc": "3.2.4",
    "zod": "4.3.6"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "dependencies": {
    "modern-normalize": "3.0.1"
  }
}
