{
  "name": "srcdev-nuxt-components",
  "type": "module",
  "version": "6.1.15",
  "main": "nuxt.config.ts",
  "scripts": {
    "clean": "rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output",
    "cleanall": "rm -rf node_modules && rm -rf .nuxt && rm -rf .output && rm -rf .playground/.nuxt && rm -rf .playground/.output && rm -rf .playground/node_modules && rm package-lock.json",
    "reinstall": "rm -rf node_modules && npm install",
    "cleaninstall": "npm run clean && npm run reinstall",
    "dev": "nuxi dev .playground",
    "build": "nuxt build .playground",
    "generate": "nuxt generate .playground",
    "preview": "nuxt preview .playground",
    "lint": "eslint .",
    "postinstall": "nuxt prepare .playground",
    "release": "release-it",
    "test": "vitest"
  },
  "files": [
    "app/",
    "nuxt.config.ts"
  ],
  "devDependencies": {
    "@iconify-json/akar-icons": "1.2.7",
    "@iconify-json/bitcoin-icons": "1.2.4",
    "@iconify-json/material-symbols": "1.2.35",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxtjs/i18n": "10.1.0",
    "@vueuse/core": "13.9.0",
    "eslint": "9.35.0",
    "focus-trap-vue": "4.1.0",
    "happy-dom": "18.0.1",
    "nuxt": "4.1.2",
    "release-it": "19.0.4",
    "typescript": "5.9.2"
  },
  "release-it": {
    "$schema": "https://unpkg.com/release-it/schema/release-it.json",
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
